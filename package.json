{
  "name": "adaptic-backend",
  "version": "1.0.134",
  "description": "A highly scalable backend solution using Prisma, Apollo Server, and TypeGraphQL, featuring GraphQL and RESTful APIs with real-time subscriptions and robust authentication.",
  "main": "dist/server.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf src/generated",
    "dev": "npm run migrate:dev && npm run fix-imports && nodemon --exec ts-node src/server.ts",
    "build": "npm run clean && npm run generate && npm run fix-imports && npm run generate:selections && npm run generate:functions && npm run generate:strings && tsc && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:package": "npm run build && npm run prepare-package-folder",
    "start": "npm run migrate && node dist/server.js",
    "generate": "SKIP_PRISMA_VERSION_CHECK=true npx prisma generate --no-engine",
    "migrate:dev": "npx prisma migrate dev && npm run migrate",
    "migrate": "npx prisma migrate deploy",
    "update-backend-functions": "node update-backend-functions.mjs",
    "generate:functions": "ts-node src/modules/index.ts",
    "generate:strings": "ts-node src/modules/generateStrings.ts",
    "generate:selections": "ts-node src/modules/generateSelections.ts",
    "fix-imports": "node fix-import-paths.cjs",
    "prepare-package-folder": "node prepare-package.mjs"
  },
  "keywords": [],
  "author": "Mehran",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.11",
    "@types/pluralize": "^0.0.33",
    "@types/ws": "^8.5.11",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@apollo/server": "^4.10.4",
    "@prisma/client": "^6.0.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@prisma/extension-pulse": "^1.2.0",
    "@prisma/generator-helper": "^6.0.0",
    "@prisma/internals": "^6.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-ws": "^5.16.0",
    "jsonwebtoken": "^9.0.2",
    "pluralize": "^8.0.0",
    "prisma": "^6.0.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "typegraphql-prisma": "^0.28.0",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "@aws-sdk/client-lambda": "^3.624.0"
  },
  "overrides": {
    "@prisma/client": "^6.0.0",
    "prisma": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
