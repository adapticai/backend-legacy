{
  "name": "@adaptic/backend-legacy",
  "version": "0.0.54",
  "description": "Backend executable CRUD functions with dynamic variables construction, and type definitions for the Adaptic AI platform.",
  "type": "module",
  "types": "index.d.ts",
  "main": "index.cjs",
  "module": "esm/index.mjs",
  "scripts": {
    "build": "tsc --declaration --emitDeclarationOnly"
  },
  "keywords": [],
  "author": "Adaptic Team",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "generated/typegraphql-prisma",
    "generated/typeStrings",
    "esm/",
    "server/",
    "resolvers/",
    "*.d.ts",
    "*.js",
    "*.cjs",
    "*.mjs",
    "*.ts",
    "*.map.js"
  ],
  "peerDependencies": {
    "@apollo/client": "^3.11.0",
    "graphql": "^16.0.0",
    "type-graphql": "^2.0.0-rc.1"
  },
  "peerDependenciesMeta": {
    "@apollo/client": {
      "optional": false
    },
    "graphql": {
      "optional": false
    },
    "type-graphql": {
      "optional": false
    }
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "reflect-metadata": "^0.2.2",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "prisma": "^5.17.0",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "always-build-npm": false,
  "notes": "Set always-build-npm to true if you want to build the package with every push regardless of changes to code (e.g. just to push a new version with some config changes)",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    }
  }
}
