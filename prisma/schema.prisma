  exitValue Decimal? @db.Decimal(12, 2)

  /// Timestamp when position was closed (null if still open)
  exitTime DateTime?

  /// Current market price (for open positions)
  currentPrice Decimal? @db.Decimal(10, 4)

  /// Current position value
  currentValue Decimal? @db.Decimal(12, 2)

  /// Unrealized P&L (for open positions)
  unrealizedPnL Decimal? @db.Decimal(12, 2)

  /// Unrealized P&L percentage
  unrealizedPnLPercent Decimal? @db.Decimal(8, 4)

  /// Realized P&L (for closed positions)
  realizedPnL Decimal? @db.Decimal(12, 2)

  /// Realized P&L percentage
  realizedPnLPercent Decimal? @db.Decimal(8, 4)

  /// Total fees and commissions paid
  totalFees Decimal @default(0) @db.Decimal(10, 2)

  /// Current Greeks for the position (snapshot)
  currentDelta Decimal? @db.Decimal(8, 6)
  currentGamma Decimal? @db.Decimal(8, 6)
  currentTheta Decimal? @db.Decimal(8, 6)
  currentVega  Decimal? @db.Decimal(8, 6)
  currentRho   Decimal? @db.Decimal(8, 6)

  /// Current implied volatility
  currentImpliedVolatility Decimal? @db.Decimal(8, 6)

  /// Days held (calculated)
  daysHeld Int?

  /// Exit reason or notes
  exitReason String?

  /// Strategy type (e.g., "covered_call", "protective_put", "vertical_spread")
  strategyType String?

  /// Reference to parent trade if linked
  tradeId String? @db.Uuid

  /// Additional metadata as JSON
  metadata Json?

  /// Timestamp when the position record was created. TYPESTRING.SKIP=true
  createdAt DateTime @default(now())

  /// Timestamp when the position record was last updated. TYPESTRING.SKIP=true
  updatedAt DateTime @updatedAt

  /// List of trade executions for this position. TYPESTRING.SKIP=true GQL.EXCLUDE=['position']
  executions OptionsTradeExecution[]

  /// Indexes for performance optimization
  @@index([alpacaAccountId, status])
  @@index([contractId])
  @@index([status])
  @@index([entryTime])
  @@index([tradeId])
  @@map("options_positions")
}

// Time-series Greeks data for options contracts
model OptionsGreeksHistory {
  /// Unique identifier for the Greeks history record. TYPESTRING.SKIP=true
  id String @id @default(uuid()) @db.Uuid

  /// Reference to the options contract
  contractId String @db.Uuid

  /// Relation to OptionsContract. TYPESTRING.SKIP=true GQL.EXCLUDE=['greeksHistory']
  contract OptionsContract @relation(fields: [contractId], references: [id], onDelete: Cascade)

  /// Timestamp of the Greeks snapshot
  timestamp DateTime

  /// Underlying asset price at this timestamp
  underlyingPrice Decimal @db.Decimal(10, 4)

  /// Option price at this timestamp
  optionPrice Decimal @db.Decimal(10, 4)

  /// Bid price
  bidPrice Decimal? @db.Decimal(10, 4)

  /// Ask price
  askPrice Decimal? @db.Decimal(10, 4)

  /// Implied volatility
  impliedVolatility Decimal @db.Decimal(8, 6)

  /// Delta Greek
  delta Decimal @db.Decimal(8, 6)

  /// Gamma Greek
  gamma Decimal @db.Decimal(8, 6)

  /// Theta Greek
  theta Decimal @db.Decimal(8, 6)

  /// Vega Greek
  vega Decimal @db.Decimal(8, 6)

  /// Rho Greek
  rho Decimal @db.Decimal(8, 6)

  /// Trading volume
  volume Int?

  /// Open interest
  openInterest Int?

  /// Days to expiration at this timestamp
  daysToExpiration Int

  /// Intrinsic value
  intrinsicValue Decimal @db.Decimal(10, 4)

  /// Extrinsic value
  extrinsicValue Decimal @db.Decimal(10, 4)

  /// Additional metadata as JSON
  metadata Json?

  /// Timestamp when the record was created. TYPESTRING.SKIP=true
  createdAt DateTime @default(now())

  /// Indexes for performance optimization
  @@unique([contractId, timestamp])
  @@index([contractId, timestamp])
  @@index([timestamp])
  @@map("options_greeks_history")
}

// Portfolio-level Greeks tracking for multi-position analysis
model PortfolioGreeksHistory {
  /// Unique identifier for the portfolio Greeks history record
  id String @id @default(uuid()) @db.Uuid

  /// Account identifier for multi-account support
  accountId String @db.Uuid

  /// Timestamp of the Greeks snapshot
  timestamp DateTime

  // Individual contract data (when contractId is provided)
  /// Optional reference to specific contract
  contractId String? @db.Uuid

  /// Option contract symbol (if contract-specific)
  symbol String?

  /// Underlying asset symbol
  underlying String

  /// Individual Greeks for contract or position
  delta Decimal @db.Decimal(8, 6)
  gamma Decimal @db.Decimal(8, 6)
  theta Decimal @db.Decimal(8, 6)
  vega  Decimal @db.Decimal(8, 6)
  rho   Decimal @db.Decimal(8, 6)

  // Portfolio-level aggregates
  /// Total portfolio delta exposure
  totalDelta Decimal? @db.Decimal(10, 6)

  /// Total portfolio gamma exposure
  totalGamma Decimal? @db.Decimal(10, 6)

  /// Total portfolio theta exposure
  totalTheta Decimal? @db.Decimal(10, 6)

  /// Total portfolio vega exposure
  totalVega Decimal? @db.Decimal(10, 6)

  /// Total portfolio rho exposure
  totalRho Decimal? @db.Decimal(10, 6)

  // Portfolio metadata
  /// Number of positions in portfolio
  positionCount Int?

  /// List of underlying symbols in portfolio
  underlyingSymbols Json?

  /// List of expiration dates in portfolio
  expirationDates Json?

  // Market context
  /// Whether recorded during market hours
  marketHours Boolean?

  /// VIX level at timestamp
  vix Decimal? @db.Decimal(8, 4)

  /// SPY price at timestamp
  spyPrice Decimal? @db.Decimal(10, 4)

  // Data provenance
  /// Data source identifier
  source String?

  /// Record creation timestamp
  createdAt DateTime @default(now())

  @@unique([accountId, timestamp, contractId])
  @@index([accountId, timestamp])
  @@index([timestamp])
  @@index([underlying])
  @@map("portfolio_greeks_history")
}

// Stores individual options trade execution details
model OptionsTradeExecution {
  /// Unique identifier for the trade execution. TYPESTRING.SKIP=true
  id String @id @default(uuid()) @db.Uuid

  /// Reference to the options position
  positionId String @db.Uuid

  /// Relation to OptionsPosition. TYPESTRING.SKIP=true GQL.EXCLUDE=['executions']
  position OptionsPosition @relation(fields: [positionId], references: [id])

  /// Reference to the options contract
  contractId String @db.Uuid

  /// Relation to OptionsContract. TYPESTRING.SKIP=true GQL.EXCLUDE=['executions']
  contract OptionsContract @relation(fields: [contractId], references: [id])

  /// Reference to the Alpaca account
  alpacaAccountId String @db.Uuid

  /// Broker order ID
  brokerOrderId String?

  /// Execution side (BUY_TO_OPEN, SELL_TO_CLOSE, etc.)
  executionSide OptionExecutionSide

  /// Number of contracts executed
  quantity Int

  /// Execution price per contract
  executionPrice Decimal @db.Decimal(10, 4)

  /// Total execution value (quantity * executionPrice * 100)
  executionValue Decimal @db.Decimal(12, 2)

  /// Fees and commissions for this execution
  fees Decimal @default(0) @db.Decimal(10, 2)

  /// Timestamp of the execution
  executionTime DateTime

  /// Underlying price at execution time
  underlyingPriceAtExecution Decimal? @db.Decimal(10, 4)

  /// Greeks at execution time
  deltaAtExecution Decimal? @db.Decimal(8, 6)
  gammaAtExecution Decimal? @db.Decimal(8, 6)
  thetaAtExecution Decimal? @db.Decimal(8, 6)
  vegaAtExecution  Decimal? @db.Decimal(8, 6)
  rhoAtExecution   Decimal? @db.Decimal(8, 6)

  /// Implied volatility at execution
  impliedVolatilityAtExecution Decimal? @db.Decimal(8, 6)

  /// Order type (MARKET, LIMIT, STOP, etc.)
  orderType String?

  /// Limit price if limit order
  limitPrice Decimal? @db.Decimal(10, 4)

  /// Stop price if stop order
  stopPrice Decimal? @db.Decimal(10, 4)

  /// Time in force (DAY, GTC, etc.)
  timeInForce String?

  /// Execution venue
  venue String?

  /// Slippage amount (difference from expected price)
  slippage Decimal? @db.Decimal(10, 4)

  /// Execution notes or comments
  notes String?

  /// Additional metadata as JSON
  metadata Json?

  /// Timestamp when the execution record was created. TYPESTRING.SKIP=true
  createdAt DateTime @default(now())

  /// Timestamp when the execution record was last updated. TYPESTRING.SKIP=true
  updatedAt DateTime @updatedAt

  /// Indexes for performance optimization
  @@index([positionId])
  @@index([contractId])
  @@index([alpacaAccountId, executionTime])
  @@index([executionTime])
  @@index([brokerOrderId])
  @@map("options_trade_executions")
}
